<!DOCTYPE html>
<html>
  <head>
	<title>Login Nat</title>
	<link rel="stylesheet" href="login.css" media="screen">
  </head>


<body>
<div class="wrapper fadeInDown">
	<div id="formContent">
	  <!-- Tabs Titles -->
	  <h2 class="active"> Sign In </h2>
	  <h2  class="inactive underlineHover"><a href='/signup'>Sign Up</a> </h2>
  
	  <!-- Icon -->
	  <div class="fadeIn first">
		<img src="images/voro-white-bg.png" id="icon" alt="User Icon" />
	  </div>
  
	  <!-- Login Form -->
	  <form action="/auth" method="post">
		<input type="text" id="login" class="fadeIn second" name="username" placeholder="Email">
		<input type="password" id="password" class="fadeIn third" name="password" placeholder="Password">
		<input type="submit" class="fadeIn fourth" value="Log In">
	  </form>
  
	</div>
  </div>
</body>
</html>